e() + 0.5em&#10;">apiVersion: apps/v1
kind: Deployment
metadata:
  name: doctor-app
  namespace: cancerns
spec:
  replicas: 1
  selector:
    matchLabels:
      app: docapi
  template:
    metadata:
      labels:
        app: docapi
    spec:
      containers:
        - name: doccontainer
          image: abhinav332/doctor-app:v5
