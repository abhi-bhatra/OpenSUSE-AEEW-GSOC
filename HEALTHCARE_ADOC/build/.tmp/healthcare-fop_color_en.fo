          claimName: data-pvc
      - name: dataset
        persistentVolumeClaim:
          claimName: ds-pvc</fo:block><fo:list-block id="id-1.9.7.6.17" space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em" space-after.optimum="1em" space-after.minimum="0.8em" space-after.maximum="1.2em" provisional-label-separation="2mm" provisional-distance-between-starts="9.5mm"><fo:list-item id="id-1.9.7.6.17.1" space-before.optimum="0.8em" space-before.minimum="0.6em" space-before.maximum="1em" keep-with-next.within-column="always" keep-with-previous.within-column="auto"><fo:list-item-label end-indent="label-end()" text-align="end" color="rgb( 12,  50,  44)"><fo:block><fo:instream-foreign-object content-height="0.4em" alignment-baseline="alphabetic" alignment-adjust="0.175em"><svg:svg xmlns:svg="http://www.w3.org/2000/svg" width="100" height="100"><svg:circle cx="50" cy="50" r="50" stroke="none" fill="rgb( 48, 186, 120)"/></svg:svg></fo:instream-foreign-object></fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block><fo:block text-align="inherit" line-height="1.6em" widows="3" orphans="3" margin-top="0" space-before.optimum="1em" space-before.minimum="0.8em" space-before.maximum="1.2em" color="rgb( 12,  50,  44)">Service.yaml</fo:block></fo:block></fo:list-item-body></fo:list-item></fo:list-block><fo:block id="id-1.9.7.6.18" text-align="start" wrap-option="wrap" hyphenation-character="&#xA0;" font-size="8.5025pt" line-height="1.6em" space-after.minimum="inherit" orphans="2" widows="2" space-before.minimum="0.8em" space-before.optimum="1em" space-before.maximum="1.2em" space-after.optimum="1em" space-after.maximum="1.2em" hyphenate="false" white-space-collapse="false" white-space-treatment="preserve" linefeed-treatment="preserve" font-family="'DejaVu Sans Mono', DejaVuSansMono, monospace" text-transform="none" color="rgb( 12,  50,  44)" background-color="rgb(227, 248, 241)" border="none" padding="0.5em" start-indent="&#10;  inherited-property-value() + 0.5em&#10;" end-indent="&#10;  inherited-property-value() + 0.5em&#10;">apiVersion: v1
